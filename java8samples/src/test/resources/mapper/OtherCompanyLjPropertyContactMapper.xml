<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="OtherCompanyLjPropertyContactMapper">

    <select id="queryByPropertyId" resultType="OtherCompanyLjPropertyContact">
        SELECT propertyNo,  contactName,cellPhone,tel,relation FROM  LjPropertyContact WHERE propertyNo=#{propertyNo}
    </select>

</mapper>