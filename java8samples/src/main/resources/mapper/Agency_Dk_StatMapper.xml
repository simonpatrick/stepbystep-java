<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="Agency_Dk_StatMapper">

    <select id="selectByPropertyId" parameterType="map" resultType="com.dooioo.fy.model.Agency_Dk_Stat" >
        SELECT  top 1 *
        FROM    Agency_Dk_Stat WITH ( NOLOCK )
        WHERE   PropertyID = #{propertyId}
    </select>

</mapper>